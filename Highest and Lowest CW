function highAndLow(numbers){
let array = numbers.split(" ");
let lowest = array[0];
let highest = array[0];
for(let i = 0; i < array.length; i++){
  if(+array[i] < +lowest) {
    lowest = array[i];
  } else if(+array[i] > +highest) {
     highest = array[i];
  }
}
  return `${highest} ${lowest}`;
}
